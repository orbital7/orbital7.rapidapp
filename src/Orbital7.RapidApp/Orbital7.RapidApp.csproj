    <None Update="Lib\orbital7-rapidapp\dist\css\_ra-fonts-opensans.scss">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Lib\orbital7-rapidapp\dist\css\_ra-layout.scss">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Lib\orbital7-rapidapp\dist\css\_ra-mixins.scss">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Lib\orbital7-rapidapp\dist\css\_ra-propertyblocks.scss">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
