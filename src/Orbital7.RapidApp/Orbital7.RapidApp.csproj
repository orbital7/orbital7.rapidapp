    </None>
    <None Update="Lib\orbital7-rapidapp\dist\css\_ra-dashboard.scss">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Lib\orbital7-rapidapp\dist\css\_ra-datagrid.scss">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Lib\orbital7-rapidapp\dist\css\_ra-defaults.scss">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Lib\orbital7-rapidapp\dist\css\_ra-dialog.scss">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
