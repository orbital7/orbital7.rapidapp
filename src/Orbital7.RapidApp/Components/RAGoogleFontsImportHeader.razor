<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>

@foreach (var fontFamilyString in FontFamilyStrings)
{
    <link rel="preload" as="style" href="https://fonts.googleapis.com/css2?family=@fontFamilyString&display=swap">
    <link href="https://fonts.googleapis.com/css2?family=@fontFamilyString&display=swap" rel="stylesheet" media="all" onload="this.media='all'">
    <noscript><link href="https://fonts.googleapis.com/css2?family=@fontFamilyString&display=swap" rel="stylesheet"></noscript>
}


@code {

    [Parameter]
    public List<string> FontFamilyStrings { get; set; } = new();
}
