@using Orbital7.RapidApp2.SampleWebApp.Models.Pages
@model IndexModel

@section taskbar {

    @Html.RATaskbar(new List<RATaskbarItem>()
    {
        new RATaskbarItem("Overview", RATaskbarItemType.LoadPagePanel,
            "/Pages/_PanelOverview"),
        new RATaskbarItem("Scroll None", RATaskbarItemType.LoadPagePanel,
            "/Pages/_PanelScrollNone"),
        new RATaskbarItem("Scroll X", RATaskbarItemType.LoadPagePanel,
            "/Pages/_PanelScrollX"),
        new RATaskbarItem("Scroll Y", RATaskbarItemType.LoadPagePanel,
            "/Pages/_PanelScrollY"),
        new RATaskbarItem("Scroll Both", RATaskbarItemType.LoadPagePanel,
            "/Pages/_PanelScrollBoth"),
        new RATaskbarItem("Tabs", RATaskbarItemType.LoadPagePanel,
            "/Pages/_PanelTabs"),
    })

}

@Html.RAPagePanel()