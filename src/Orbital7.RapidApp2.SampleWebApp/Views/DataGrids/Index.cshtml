@using Orbital7.RapidApp2.SampleWebApp.Models.DataGrids
@model IndexModel

@section taskbar {

    @Html.RATaskbar(new List<RATaskbarItem>()
    {
        new RATaskbarItem("Overview", RATaskbarItemType.LoadPagePanel,
            "/DataGrids/_PanelOverview"),
        new RATaskbarItem("Basic", RATaskbarItemType.LoadPagePanel,
            "/DataGrids/_PanelBasic"),
        new RATaskbarItem("In Tabs", RATaskbarItemType.LoadPagePanel,
            "/DataGrids/_PanelOverview"),
        new RATaskbarItem("Selectable", RATaskbarItemType.LoadPagePanel,
            "/DataGrids/_PanelOverview"),
    })

}

@Html.RAPagePanel()